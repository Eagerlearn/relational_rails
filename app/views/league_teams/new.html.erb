<%= form_with url: "/leagues/#{@league.id}/teams", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :expansion_team %>
  <%= form.text_field :expansion_team %>
  <%= form.label :number_of_players %>
  <%= form.text_field :number_of_players %>
  <%= form.submit 'Create Team' %>
<% end %>